{
  "name": "ionic-vue-todo-list",
  "version": "1.0.0",
  "description": "Todo List application built with Ionic 7 and Vue 3",
  "author": "Your Name",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "ios": "ionic capacitor run ios -l --external",
    "android": "ionic capacitor run android -l --external",
    "build:ios": "ionic capacitor build ios",
    "build:android": "ionic capacitor build android",
    "sync": "ionic capacitor sync"
  },
  "dependencies": {
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@ionic/vue": "^7.5.6",
    "@ionic/vue-router": "^7.5.6",
    "ionicons": "^7.2.1",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@ionic/cli": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cypress": "^13.6.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
